{% capture listItems %}

{%- assign allItems = collections -%}
{%- for item in allItems -%}{% assign condition_0 = false %}                  
                {% unless item.handle == collection.handle %}
                    {% assign condition_0 = true %}
                {% endunless %}{% if condition_0 %}
 <div role="listitem" class="w-dyn-item">
            <a href="{{ item.url }}" class="inner-grid-cms vertical w-inline-block">
              <div class="product-image-wrapper home"><img src="{{ item.image | img_url: 'master' }}" loading="lazy" alt="{{ item.image.alt }}" class="image-cms" srcset="{{ item.image | img_url: '1920x' }} 1920w,{{ item.image | img_url: '1600x' }} 1600w,{{ item.image | img_url: '1366x' }} 1366w,{{ item.image | img_url: '1024x' }} 1024w,{{ item.image | img_url: '768x' }} 768w,{{ item.image | img_url: '640x' }} 640w"></div>
              <div id="w-node-bcf9455a-9d12-8a9e-94c2-12f63d52c132-aa251566" class="info-members">
                <h3 class="heading-regular">{{ item.title }}</h3>
                <p>{{ item.metafields.udesly['short-description'] }}</p>
              </div>
            </a>
          </div>
{% endif %}
{%- endfor -%}

{% endcapture %}
{% assign items_length = listItems | strip | size %}

<div class="w-dyn-list" udy-collection="collection">
        {%- unless items_length == 0 -%}<div role="list" class="collection-list-products w-dyn-items">
          {{ listItems }}
        </div>{%- endunless -%}
        {%- if items_length == 0 -%}<div class="w-dyn-empty">
          <div>No items found.</div>
        </div>{%- endif -%}
      </div>