{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "627929cccf40a999bb25155c" -%}{%- assign bodyClasses = "" -%}
  {%- render 'loader' -%}
  {%- section 'global-nav' -%}
  {%- section 'header-title' -%}
  <div class="section wf-section">
    <div class="container w-container">
      <div class="w-layout-grid main-grid margin-bottom-small">
        <div id="w-node-d06b0da3-afeb-bf64-efb8-f8b4ab472887-bb25155c" class="overflow-wrapper rounded">
          <div data-enable-scroll="true" data-enable-touch="true" data-widget-latlng="" data-widget-tooltip="" data-widget-style="roadmap" data-widget-zoom="12" title="" class="w-widget w-widget-map"></div>
        </div>
        <div id="w-node-d06b0da3-afeb-bf64-efb8-f8b4ab472889-bb25155c" class="contain-form rounded">
          <div class="form-container">
            <div class="form-wrapper left">
              <div class="full-form w-form">
                {%- form "contact",  class: "inner-form" -%}
                  <div class="inner-input">
                    <div class="label-form">Full name</div><input type="text" class="text-field dark w-input" maxlength="256" name="contact[name]" data-name="Name" placeholder="e.g. John Smith" id="Name" required="">
                  </div>
                  <div class="inner-input">
                    <div class="label-form">Email Address</div><input type="email" class="text-field dark w-input" maxlength="256" name="contact[email]" data-name="Email 3" placeholder="e.g. john@email.com" id="email-3" required="">
                  </div>
                  <div class="inner-input margin-bottom-xsmall">
                    <div class="label-form">Message</div><textarea placeholder="Message..." maxlength="5000" id="field" name="contact[field]" data-name="Field" class="textarea-message w-input"></textarea>
                  </div>
                  <div class="inner-input margin-bottom-xsmall"><input type="submit" value="Submit" data-wait="Please wait..." class="button color-red w-button"></div>
                {%- assign form_success = form.posted_successfully? -%}{%- assign form_errors = form.errors -%}{%- render "form-general-script", form_success: form_success, form_errors: form_errors -%}{%- endform -%}
                <div class="success rounded w-form-done  {% if form_success %}form-posted-successfully{% endif %}">
                  <div>Thank you! Your submission has been received!</div>
                </div>
                {%- unless form_success -%}<div class="error rounded w-form-fail  {% if form.errors %}form-has-errors{% endif %}">
                  <div>{%- render "form-errors", form_errors: form_errors -%}</div>
                </div>{%- endunless -%}
              </div>
            </div>
          </div>
        </div>
      </div>
      <div class="social-icons-wrapper">
        <a href="http://www.facebook.com" target="_blank" class="link-social w-inline-block"><img src="{{ 'ic-facebook-white.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>
        <a href="http://www.instagram.com/joaopaulots" target="_blank" class="link-social w-inline-block"><img src="{{ 'ic-instagram-white.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>
        <a href="http://www.linkedin.com" target="_blank" class="link-social w-inline-block"><img src="{{ 'ic-linkedin-white.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>
        <a href="http://www.twitter.com" target="_blank" class="link-social w-inline-block"><img src="{{ 'ic-twitter-white.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>
        <a href="#" class="link-social w-inline-block"><img src="{{ 'icon-mail.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>
      </div>
    </div>
