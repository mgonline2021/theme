{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "627929cccf40a91d7725159d" -%}{%- assign bodyClasses = "" -%}
  {%- render 'loader' -%}
  {%- section 'global-nav' -%}
  <div class="section titles wf-section">
    <div class="container titles w-container">
      <div class="heading-regular text-gray-light margin-bottom-xsmall">{{ article.metafields.udesly['job-title'] }}</div>
      <h1 class="heading-xlarge white">{{ article.title }}</h1>
      <div class="wrap-summary">
        <p class="main-paragraph bold">{{ article.metafields.udesly['bio-summary'] }}</p>
      </div>
    </div>
    <div class="container w-container">
      <div class="wrapper-full-image rounded margin-bottom-medium"><img src="{{ article.image | img_url: 'master' }}" loading="lazy" alt="{{ article.image.alt }}" class="full-image" srcset="{{ article.image | img_url: '1920x' }} 1920w,{{ article.image | img_url: '1600x' }} 1600w,{{ article.image | img_url: '1366x' }} 1366w,{{ article.image | img_url: '1024x' }} 1024w,{{ article.image | img_url: '768x' }} 768w,{{ article.image | img_url: '640x' }} 640w"></div>
      <div class="w-layout-grid main-grid">
        <div id="w-node-_3e4d38fa-5d84-f528-7e12-35f5daf5d771-7725159d" class="grid-wrapper padding-bottom-medium">
          <div class="w-richtext">{{ article.content }}</div>
        </div>
      </div>
      <div class="social-icons-wrapper">
        {%- if article.metafields.udesly['facebook-link'] -%}<a href="{{ article.metafields.udesly['facebook-link'] }}" class="link-social w-inline-block"><img src="{{ 'ic-facebook-white.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>{%- endif -%}
        {%- if article.metafields.udesly['instagram-link'] -%}<a href="{{ article.metafields.udesly['instagram-link'] }}" class="link-social w-inline-block"><img src="{{ 'ic-instagram-white.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>{%- endif -%}
        {%- if article.metafields.udesly['facebook-link'] -%}<a href="{{ article.metafields.udesly['linkedin-link'] }}" class="link-social w-inline-block"><img src="{{ 'ic-linkedin-white.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>{%- endif -%}
        {%- if article.metafields.udesly['twitter-link'] -%}<a href="{{ article.metafields.udesly['twitter-link'] }}" class="link-social w-inline-block"><img src="{{ 'ic-twitter-white.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>{%- endif -%}
        {%- if article.metafields.udesly['email'] -%}<a href="{{ article }}" class="link-social w-inline-block"><img src="{{ 'icon-mail.svg' | asset_url }}" loading="lazy" alt="" class="icon-social profile"></a>{%- endif -%}
      </div>
    </div>
  </div>
  <div class="section wf-section">
    <div class="container inner-padding-medium w-container">
      <div class="heading-wrapper-center inner-padding-small">
        <h2 class="text">Team Members</h2>
      </div>
    </div>
  </div>
  <div class="section wf-section">
    <div class="container inner-padding-small w-container">
      {%- render 'call-to-action-gray' -%}
    </div>
  </div>
  {%- render 'footer' -%}
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=627929cccf40a94a9f251539" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
