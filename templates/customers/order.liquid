{%- assign headerSnippetPath = "header-index" -%}{%- assign wfPage = "627929cccf40a96d6d2515e7" -%}{%- assign bodyClasses = "" -%}
  <div class="section light-blue wf-section">
    <div class="wrapper-header">
      <div class="w-container">
        <div class="paragraph-wrapper inner-padding-small">
          <a href="{{ routes.root_url }}" class="wrapper-link-back w-inline-block"><img src="{{ 'untitled-design-35.png' | asset_url }}" loading="lazy" alt="" class="image-logo-back">
            <div class="text-block-17">â€¹ Torna Indietro</div>
          </a>
          <h1 class="heading-large2 merch text-black paragraph-wrapper">Ordine Confermato.</h1>
        </div>
      </div>
    </div>
    <div data-node-type="commerce-order-confirmation-wrapper" data-wf-order-query="" data-wf-page-link-href-prefix="" class="w-commerce-commerceorderconfirmationcontainer order-confirmation">
      <div class="w-commerce-commercelayoutcontainer w-container">
        <div class="w-commerce-commercelayoutmain">
          <div class="w-commerce-commercecheckoutcustomerinfosummarywrapper wrap-form-checkout">
            <div class="w-commerce-commercecheckoutsummaryblockheader">
              <h4 class="heading-small-new text-gray-light">Customer Information</h4>
            </div>
            <fieldset class="w-commerce-commercecheckoutblockcontent">
              <div class="w-commerce-commercecheckoutrow">
                <div class="w-commerce-commercecheckoutcolumn">
                  <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel">Email</label>
                    <div>{{ order.email }}</div>
                  </div>
                </div>
                {%- if order.shipping_methods.size > 0 -%}<div class="w-commerce-commercecheckoutcolumn">
                  <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel">Shipping Address</label>
                    <div>{{ order.shipping_address.name }}</div>
                    <div>{{ order.shipping_address.address1 }}</div>
                    <div>{{ order.shipping_address.address2 }}</div>
                    <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                      <div class="w-commerce-commercecheckoutsummarytextspacingondiv">{{ order.shipping_address.city }}</div>
                      <div class="w-commerce-commercecheckoutsummarytextspacingondiv">{{ order.shipping_address.state }}</div>
                      <div class="w-commerce-commercecheckoutsummarytextspacingondiv">{{ order.shipping_address.zip }}</div>
                    </div>
                    <div>{{ order.shipping_address.country }}</div>
                  </div>
                </div>{%- endif -%}
              </div>
            </fieldset>
          </div>
          {%- if order.shipping_methods.size > 0 -%}<div class="w-commerce-commercecheckoutshippingsummarywrapper wrap-form-checkout">
            <div class="w-commerce-commercecheckoutsummaryblockheader">
              <h4 class="heading-small-new text-gray-light">Shipping Method</h4>
            </div>
            <fieldset class="w-commerce-commercecheckoutblockcontent">
              <div class="w-commerce-commercecheckoutrow">
                <div class="w-commerce-commercecheckoutcolumn">
                  <div class="w-commerce-commercecheckoutsummaryitem">
                    <div>{{ order.shipping_methods[0].title }}</div>
                    
                  </div>
                </div>
              </div>
            </fieldset>
          </div>{%- endif -%}
          <div class="w-commerce-commercecheckoutpaymentsummarywrapper wrap-form-checkout">
            <div class="w-commerce-commercecheckoutsummaryblockheader">
              <h4 class="heading-small-new text-gray-light">Payment Info</h4>
            </div>
            <fieldset class="w-commerce-commercecheckoutblockcontent">
              <div class="w-commerce-commercecheckoutrow">
                <div class="w-commerce-commercecheckoutcolumn">
                  <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel">Payment Info</label>
                    <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                      <div class="w-commerce-commercecheckoutsummarytextspacingondiv">{{ order.transactions[0].payment_details.credit_card_company }}</div>
                      <div class="w-commerce-commercecheckoutsummarytextspacingondiv">{{ order.transactions[0].payment_details.credit_card_number }}</div>
                    </div>
                    <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                      
                      {%- if order. -%}<div> / </div>{%- endif -%}
                      
                    </div>
                  </div>
                </div>
                <div class="w-commerce-commercecheckoutcolumn">
                  <div class="w-commerce-commercecheckoutsummaryitem"><label class="w-commerce-commercecheckoutsummarylabel">Billing Address</label>
                    <div>{{ order.billing_address.name }}</div>
                    <div>{{ order.billing_address.address1 }}</div>
                    <div>{{ order.billing_address.address2 }}</div>
                    <div class="w-commerce-commercecheckoutsummaryflexboxdiv">
                      <div class="w-commerce-commercecheckoutsummarytextspacingondiv">{{ order.billing_address.city }}</div>
                      <div class="w-commerce-commercecheckoutsummarytextspacingondiv">{{ order.billing_address.state }}</div>
                      <div class="w-commerce-commercecheckoutsummarytextspacingondiv">{{ order.billing_address.zip }}</div>
                    </div>
                    <div>{{ order.billing_address.country }}</div>
                  </div>
                </div>
              </div>
            </fieldset>
          </div>
          <div class="w-commerce-commercecheckoutorderitemswrapper wrap-form-checkout">
            <div class="w-commerce-commercecheckoutsummaryblockheader">
              <h4 class="heading-small-new text-gray-light">Items in Order</h4>
            </div>
            <fieldset class="w-commerce-commercecheckoutblockcontent">
              <script type="text/x-wf-template" id="wf-template-60e53327882bce59ee71146e000000000051"><div data-node-type="checkout-order-item" class="w-commerce-commercecheckoutorderitem"><img data-node-type="cart-item-image" template-part-bind="60e53327882bce59ee71146e000000000053" class="w-commerce-commercecartitemimage" src="<%= it.image %>"><div data-node-type="checkout-order-item-description-wrapper" class="w-commerce-commercecheckoutorderitemdescriptionwrapper"><div data-node-type="cart-product-name" template-part-bind="60e53327882bce59ee71146e000000000055" class="w-commerce-commercecartproductname"><%= it.title %></div><div data-node-type="checkout-order-item-quantity-wrapper" class="w-commerce-commercecheckoutorderitemquantitywrapper"><div>Quantity: </div><div template-part-bind="60e53327882bce59ee71146e000000000059"><%= it.count %></div></div><ul data-node-type="checkout-order-item-option-list" class="w-commerce-commercecheckoutoptionlist"><% Object.entries(it.properties).forEach( data =>  { %><li data-node-type="checkout-order-item-option-list-item"><span data-node-type="checkout-order-item-option-list-item-label"><%= data[0] %></span><span>: </span><span data-node-type="checkout-order-item-option-list-item-value"><%= data[1] %></span></li><% }) %></ul></div><div template-part-bind="60e53327882bce59ee71146e000000000060"><%= it.rowTotal %></div></div></script>
              <div class="w-commerce-commercecheckoutorderitemslist" data-wf-collection="database.commerceOrder.userItems" data-wf-template-id="wf-template-60e53327882bce59ee71146e000000000051">{%- render 'order-items' -%}</div>
            </fieldset>
          </div>
        </div>
        <div class="w-commerce-commercelayoutsidebar">
          <div class="w-commerce-commercecheckoutordersummarywrapper wrap-form-checkout">
            <div class="w-commerce-commercecheckoutsummaryblockheader">
              <h4 class="heading-small-new text-gray-light">Order Summary</h4>
            </div>
            <fieldset class="w-commerce-commercecheckoutblockcontent">
              <div class="w-commerce-commercecheckoutsummarylineitem">
                <div>Subtotal</div>
                <div>{{ order.subtotal_price | money }}</div>
              </div>
              <script type="text/x-wf-template" id="wf-template-60e53327882bce59ee71146e00000000006b">%3Cdiv%20class%3D%22w-commerce-commercecheckoutordersummaryextraitemslistitem%22%3E%3Cdiv%20bind%3D%2260e53327882bce59ee71146e00000000006d%22%3E%3C%2Fdiv%3E%3Cdiv%20bind%3D%2260e53327882bce59ee71146e00000000006e%22%3E%3C%2Fdiv%3E%3C%2Fdiv%3E</script>
              <div class="w-commerce-commercecheckoutordersummaryextraitemslist" data-wf-collection="database.commerceOrder.extraItems" data-wf-template-id="wf-template-60e53327882bce59ee71146e00000000006b">{%- for tax_line in order.tax_lines -%}<div class="w-commerce-commercecheckoutordersummaryextraitemslistitem"><div>{{ tax_line.title }} ({{ tax_line.rate | times: 100 }}%): </div><div>{{ tax_line.price | money }}</div></div>{%- endfor -%}</div>
              <div class="w-commerce-commercecheckoutsummarylineitem">
                <div>Total</div>
                <div class="w-commerce-commercecheckoutsummarytotal">{{ order.total_price | money }}</div>
              </div>
            </fieldset>
          </div>
        </div>
      </div>
    </div>
  </div>
  {%- render 'footer' -%}
  <script src="https://d3e54v103j8qbb.cloudfront.net/js/jquery-3.5.1.min.dc5e7f18c8.js?site=627929cccf40a94a9f251539" type="text/javascript" integrity="sha256-9/aliU8dGd2tb6OSsuzixeV4y/faTqgFtohetphbbj0=" crossorigin="anonymous"></script>
  <script src="{{ 'webflow.js' | asset_url }}" type="text/javascript"></script>
  <!-- [if lte IE 9]><script src="https://cdnjs.cloudflare.com/ajax/libs/placeholders/3.0.2/placeholders.min.js"></script><![endif] -->
  <style>
  .w-webflow-badge {
        position: fixed !important;
    display: None !important;
    visibility: hidden !important;
}
<style>
</body></html></style>{{ 'udesly-shopify.min.js' | asset_url | script_tag }}