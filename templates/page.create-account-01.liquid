                {%- form "contact",  class: "inner-form" -%}
                  <div class="inner-input">
                    <div class="label-form">Full name</div><input type="text" class="text-field dark w-input" maxlength="256" name="contact[name]" data-name="Name" placeholder="Enter your full name" id="Name" required="">
                  </div>
                  <div class="inner-input">
                    <div class="label-form">Your Email</div><input type="email" class="text-field dark w-input" maxlength="256" name="contact[email]" data-name="Email" placeholder="Enter your email" id="Email-4" required="">
